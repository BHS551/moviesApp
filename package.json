{
  "name": "moviesapp",
  "version": "1.0.0",
  "description": "Server used to provide movies information to front-end aplications",
  "main": "server.js",
  "scripts": {
    "lint": "eslint src/**/*.js --fix",
    "format": "prettier-eslint src/**/*.{js,json} --write",
    "test": "jest --passWithNoTests --testPathPattern=$TEST_PATH",
    "dev": "nodemon --exec babel-node server.js",
    "build": "webpack"
  },
  "lint-staged": {
    "*.js": [
      "eslint src/**/*.js --fix",
      "prettier-eslint src/**/*.{js,json} --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^3.0.5",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.3",
    "nodemon-webpack-plugin": "^4.5.2",
    "prettier": "^2.3.2",
    "prettier-eslint-cli": "^5.0.0",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1"
  }
}
